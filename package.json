{
  "name": "@terrestris/shogun-gis-client",
  "version": "8.11.0",
  "description": "This is the default GIS client used in the SHOGun project. Based on the react-geo-client template",
  "keywords": [
    "shogun",
    "react",
    "react-geo",
    "ol",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:terrestris/shogun-gis-client.git"
  },
  "license": "BSD-2-Clause",
  "author": "terrestris GmbH & Co. KG <info@terrestris.de>",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rspack --config rspack.prod.js build",
    "build:analyze": "npm run build -- --analyze",
    "build:babel": "BABEL_ENV=production babel src --out-dir dist --copy-files --no-copy-ignored --ignore 'src/**/*.spec.tsx' --ignore 'src/**/*.spec.js' --extensions '.ts,.tsx' --source-maps",
    "build:declaration": "tsc --emitDeclarationOnly",
    "build:dist": "npm run clean:dist && npm run build:babel && npm run build:declaration",
    "check": "npm run typecheck && npm run lint && npm run test",
    "clean:dist": "rimraf ./dist/* ./build/*",
    "husky": "husky",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx src/",
    "lint:fix": "eslint -c .eslintrc.js --ext .ts,.tsx src/ --fix",
    "prepare": "npm run husky",
    "semantic-release": "npm run clean; npm run build; npx semantic-release",
    "start": "rspack serve --config rspack.dev.js",
    "test": "jest --maxWorkers=50% --coverage",
    "tsc": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@reduxjs/toolkit": "^2.2.5",
    "@terrestris/base-util": "^2.0.0",
    "@terrestris/mapfish-print-manager": "^14.0.0",
    "@terrestris/ol-util": "^19.0.0",
    "@terrestris/react-geo": "24.3.0",
    "@terrestris/react-util": "^8.0.5",
    "@terrestris/shogun-e2e-tests": "^1.0.8",
    "@terrestris/shogun-util": "^8.3.1",
    "antd": "^5.17.4",
    "color": "^4.2.3",
    "dotenv": "^16.4.5",
    "geostyler": "^15.0.0",
    "geostyler-openlayers-parser": "^5.0.0",
    "geostyler-style": "^9.1.0",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "js-md5": "^0.8.3",
    "keycloak-js": "^25.0.1",
    "normalize.css": "^8.0.1",
    "ol": "^9.2.4",
    "react": "^18.3.1",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.0",
    "react-redux": "^9.1.2",
    "shapefile.js": "^1.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.6",
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@babel/runtime": "^7.24.6",
    "@casualbot/jest-sonar-reporter": "^2.3.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@module-federation/enhanced": "^0.5.0",
    "@playwright/test": "^1.44.1",
    "@rspack/cli": "^0.7.5",
    "@rspack/core": "^0.7.5",
    "@rspack/plugin-react-refresh": "^1.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.5",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@swc/helpers": "^0.5.11",
    "@terrestris/eslint-config-typescript": "^5.0.0",
    "@terrestris/eslint-config-typescript-react": "^2.0.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/color": "^3.0.6",
    "@types/jest": "^29.5.12",
    "@types/js-md5": "^0.7.2",
    "@types/node": "^22.0.2",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.28.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.2",
    "fs-extra": "11.2.0",
    "husky": "^9.0.11",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "mini-css-extract-plugin": "^2.9.0",
    "path-exists-cli": "2.0.0",
    "react-refresh": "^0.14.2",
    "semantic-release": "^24.0.0",
    "style-loader": "^4.0.0",
    "typescript": "^5.4.5",
    "watch": "1.0.2",
    "webpack-merge": "^6.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "@casualbot/jest-sonar-reporter": {
    "suiteName": "jest tests",
    "outputDirectory": "coverage",
    "outputName": "jest-report.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true",
    "relativePaths": "true"
  },
  "devEngines": {
    "node": ">=20",
    "npm": ">=7"
  }
}
