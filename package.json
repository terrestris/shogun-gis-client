{
  "name": "@terrestris/shogun-gis-client",
  "version": "4.2.1",
  "description": "This is the default GIS client used in the SHOGun project. Based on the react-geo-client template",
  "keywords": [
    "shogun",
    "react",
    "react-geo",
    "ol",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:terrestris/shogun-gis-client.git"
  },
  "license": "BSD-2-Clause",
  "author": "terrestris GmbH & Co. KG <info@terrestris.de>",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:declaration": "tsc --emitDeclarationOnly",
    "build": "webpack --config webpack.prod.js",
    "build:dist": "npm run clean:dist && BABEL_ENV=production babel src --out-dir dist --copy-files --ignore spec.tsx,.md --extensions '.ts,.tsx' --source-maps && npm run build:declaration",
    "clean:dist": "rimraf ./dist/*",
    "build:analyze": "BUNDLE_ANALYZE=true npm run build",
    "check": "npm run typecheck && npm run lint && npm run test && npm run test:ui",
    "husky": "husky install",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx src/",
    "prepare": "npm run husky",
    "semantic-release": "npm run clean; npm run build; npx semantic-release",
    "start": "webpack serve --config webpack.dev.js",
    "test": "jest --coverage",
    "test:ui": "playwright test",
    "tsc": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "watch:buildto": "node watchBuild.js"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "@terrestris/base-util": "^1.0.1",
    "@terrestris/mapfish-print-manager": "^6.5.1",
    "@terrestris/ol-util": "^7.4.0",
    "@terrestris/react-util": "^2.1.1",
    "@terrestris/react-geo": "^19.4.0",
    "@terrestris/shogun-util": "^3.4.1",
    "antd": "^4.22.7",
    "color": "^4.2.3",
    "i18next": "^21.9.1",
    "i18next-browser-languagedetector": "^6.1.5",
    "js-md5": "^0.7.3",
    "keycloak-js": "^19.0.1",
    "normalize.css": "^8.0.1",
    "ol": "^6.15.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.18.5",
    "react-redux": "^8.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.18.6",
    "@casualbot/jest-sonar-reporter": "^2.2.5",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@playwright/test": "^1.25.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.5",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@terrestris/eslint-config-typescript": "^3.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@types/color": "^3.0.3",
    "@types/jest": "^29.0.0",
    "@types/js-md5": "^0.4.3",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "babel-jest": "^29.0.0",
    "babel-loader": "^8.2.5",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.22.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "fs-extra": "10.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "ignore-loader": "^0.1.2",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "path-exists-cli": "2.0.0",
    "react-refresh": "0.14.0",
    "semantic-release": "^19.0.4",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.5",
    "typescript": "^4.7.4",
    "watch": "1.0.2",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devEngines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "@casualbot/jest-sonar-reporter": {
    "suiteName": "jest tests",
    "outputDirectory": "coverage",
    "outputName": "jest-report.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true",
    "relativePaths": "true"
  }
}
