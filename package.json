{
  "name": "@terrestris/shogun-gis-client",
  "version": "6.5.0",
  "description": "This is the default GIS client used in the SHOGun project. Based on the react-geo-client template",
  "keywords": [
    "shogun",
    "react",
    "react-geo",
    "ol",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:terrestris/shogun-gis-client.git"
  },
  "license": "BSD-2-Clause",
  "author": "terrestris GmbH & Co. KG <info@terrestris.de>",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:analyze": "BUNDLE_ANALYZE=true npm run build",
    "build:babel": "BABEL_ENV=production babel src --out-dir dist --copy-files --no-copy-ignored --ignore 'src/**/*.spec.tsx' --ignore 'src/**/*.spec.js' --extensions '.ts,.tsx' --source-maps",
    "build:declaration": "tsc --emitDeclarationOnly",
    "build:dist": "npm run clean:dist && npm run build:babel && npm run build:declaration",
    "check": "npm run typecheck && npm run lint && npm run test",
    "clean:dist": "rimraf ./dist/*",
    "husky": "husky install",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx src/",
    "prepare": "npm run husky",
    "semantic-release": "npm run clean; npm run build; npx semantic-release",
    "start": "webpack serve --config webpack.dev.js",
    "test": "jest --coverage",
    "test:ui": "playwright test",
    "tsc": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "watch:buildto": "node watchBuild.js"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "@terrestris/base-util": "^1.0.1",
    "@terrestris/mapfish-print-manager": "^9.0.1",
    "@terrestris/ol-util": "^10.1.3",
    "@terrestris/react-geo": "^22.1.0",
    "@terrestris/react-util": "^2.1.1",
    "@terrestris/shogun-util": "^5.5.0",
    "antd": "^4.24.4",
    "color": "^4.2.3",
    "geostyler": "^12.0.0",
    "geostyler-openlayers-parser": "^4.1.0",
    "geostyler-style": "^7.2.0",
    "i18next": "^23.0.2",
    "i18next-browser-languagedetector": "^7.0.1",
    "js-md5": "^0.7.3",
    "keycloak-js": "^21.0.0",
    "normalize.css": "^8.0.1",
    "ol": "^7.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^12.0.0",
    "react-redux": "^8.0.5",
    "shapefile.js": "^1.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.6",
    "@casualbot/jest-sonar-reporter": "^2.2.5",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@playwright/test": "^1.3.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.2",
    "@semantic-release/npm": "^10.0.2",
    "@semantic-release/release-notes-generator": "^11.0.1",
    "@terrestris/eslint-config-typescript": "^3.1.0",
    "@terrestris/eslint-config-typescript-react": "^2.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/color": "^3.0.3",
    "@types/jest": "^29.2.3",
    "@types/js-md5": "^0.7.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "eslint": "^8.28.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.11",
    "fs-extra": "11.1.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "ignore-loader": "^0.1.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.7.0",
    "path-exists-cli": "2.0.0",
    "react-refresh": "0.14.0",
    "semantic-release": "^21.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^5.0.3",
    "watch": "1.0.2",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "@casualbot/jest-sonar-reporter": {
    "suiteName": "jest tests",
    "outputDirectory": "coverage",
    "outputName": "jest-report.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true",
    "relativePaths": "true"
  },
  "devEngines": {
    "node": ">=14",
    "npm": ">=7"
  }
}
